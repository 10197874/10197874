<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameFourVM" type="com.weempowerapp.app.modules.framefour.data.viewmodel.FrameFourVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearColumnhighlights"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_34pxv"
        android:paddingEnd="@dimen/_39pxh"
        android:paddingStart="@dimen/_39pxh"
        android:paddingTop="@dimen/_34pxv" style="@style/groupStylewhite_3">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnsometechj"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_66pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="wrap_content" android:orientation="vertical">
            <com.asksira.loopingviewpager.LoopingViewPager
                android:id="@+id/imageSliderViewhierarchy"
                android:layout_height="@dimen/_336pxv"
                android:layout_width="match_parent"
                app:autoScroll="true" app:isInfinite="true" app:scrollInterval="5000"/>

            <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                android:id="@+id/indicatorUserOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_111pxh"
                android:layout_width="wrap_content"
                app:drawable_normal="@drawable/indicator_unselected_bg_gray_300_radius_8"
                app:drawable_selected="@drawable/indicator_selected_bg_deep_purple_a400_radius_8"
                app:indicator_size="@dimen/_8pxh" app:indicator_spacing="@dimen/_4pxh"/>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowhighlights"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtHighlights"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{frameFourVM.frameFourModel.txtHighlights}"
                style="@style/txtLexendbold24" tools:text="@string/lbl_highlights"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSeeall"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_102pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{frameFourVM.frameFourModel.txtSeeall}"
                style="@style/txtManroperegular14_3" tools:text="@string/lbl_see_all"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearContainer10"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_66pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfinreportco"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnfinreport"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_31pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFinreportco"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:text="@{frameFourVM.frameFourModel.txtFinreportco}"
                        style="@style/txtManropemedium7" tools:text="@string/msg_https_www_fac"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtStockmarket"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{frameFourVM.frameFourModel.txtStockmarket}"
                        style="@style/txtLexendsemibold16" tools:text="@string/msg_at_mbilwi_secondary"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageImage"
                    android:layout_height="@dimen/_84pxh"
                    android:layout_width="@dimen/_91pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_71499090_687082}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_71499090_687082"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRow2hago"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt2hago"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{frameFourVM.frameFourModel.txt2hago}"
                    style="@style/txtNotoSansnotosansitalic12" tools:text="@string/lbl_2h_ago"/>
                <ImageView android:id="@+id/imageIconbuttonfour"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginStart="@dimen/_280pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_icon_button_3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_icon_button_3"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageWhatsappimage"
            android:layout_gravity="start"
            android:layout_height="@dimen/_126pxh"
            android:layout_marginStart="@dimen/_66pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="@dimen/_228pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_whatsapp_image_2024_04_14}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_whatsapp_image_2024_04_14"/>
    </LinearLayout>
</layout>
