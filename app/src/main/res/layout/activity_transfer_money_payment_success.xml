            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_52pxv"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_342pxh"
            android:lineSpacingExtra="@dimen/_7pxh"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:text="@string/msg_make_another_payment"
            android:textAllCaps="false"
