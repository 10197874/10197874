<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="transferMoneyPaymentSuccessVM" type="com.weempowerapp.app.modules.transfermoneypaymentsuccess.data.viewmodel.TransferMoneyPaymentSuccessVM"/>
    </data>
    <LinearLayout android:id="@+id/linearTransferMoney"
