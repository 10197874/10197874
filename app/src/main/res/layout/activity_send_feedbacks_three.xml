<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="sendFeedbacksThreeVM" type="com.weempowerapp.app.modules.sendfeedbacksthree.data.viewmodel.SendFeedbacksThreeVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnrating"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearSendfeedbacks"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameContainer4"
                android:layout_height="@dimen/_147pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumncontainer"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewContainerfive"
                        android:layout_height="@dimen/_40pxv" android:layout_width="@dimen/_375pxh"/>
                    <ImageView android:id="@+id/imageButtonthreeOne"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_44pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_button_3"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_button_3"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStackrating"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_99pxv" android:layout_width="@dimen/_216pxh">
                    <ImageView android:id="@+id/imageWhatsappimage"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_whatsapp_image_2024_04_14}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_whatsapp_image_2024_04_14"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRating"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_62pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtRating}"
                        style="@style/txtLexendbold24" tools:text="@string/lbl_rating"/>
                </FrameLayout>
            </FrameLayout>
            <ImageView android:id="@+id/imageCircleimage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_116pxh"
                android:layout_width="@dimen/_116pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_rectangle_116x116}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_116x116"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMichael"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtMichael}"
                style="@style/txtLexendbold18" tools:text="@string/msg_arnold_ferdinand"/>
            <androidx.appcompat.widget.AppCompatRatingBar
                android:id="@+id/ratingBarRatingbar"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content" android:numStars="5"
                android:rating="0" style="@style/Widget.AppCompat.RatingBar.Indicator"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearTag1"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowprofessional"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_100_1">
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtProfessional"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtProfessional}"
                        style="@style/txtManroperegular14_3" tools:text="@string/lbl_professional"/>
                    <ImageView android:id="@+id/imageCheckOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_check"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_check"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowfriendly"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_6pxv" style="@style/groupStylegray_100_1">
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtFriendly"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtFriendly}"
                        style="@style/txtManroperegular14_3" tools:text="@string/lbl_friendly"/>
                    <ImageView android:id="@+id/imageCheckThree"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_check"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_check"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearTag2"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowproficient"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_100cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtProficient"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtProficient}"
                        style="@style/txtManroperegular14_4" tools:text="@string/lbl_proficient"/>
                    <ImageView android:id="@+id/imageEaddOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_e_add"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_e_add"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowproactive"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_100cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtProactive"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtProactive}"
                        style="@style/txtManroperegular14_4" tools:text="@string/lbl_proactive"/>
                    <ImageView android:id="@+id/imageEaddThree"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_e_add"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_e_add"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearTag3"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowpromotion"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_100cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/txtPromotion"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtPromotion}"
                        style="@style/txtManroperegular14_4" tools:text="@string/lbl_promotion"/>
                    <ImageView android:id="@+id/imageEaddFive"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_e_add"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_e_add"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowgift"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_12pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_100_1">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGift"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtGift}"
                        style="@style/txtManroperegular14_3" tools:text="@string/lbl_gift"/>
                    <ImageView android:id="@+id/imageCheckFive"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_check"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_check"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumncaretosha"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingEnd="@dimen/_20pxh" android:paddingStart="@dimen/_20pxh">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCaretoshare"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{sendFeedbacksThreeVM.sendFeedbacksThreeModel.txtCaretoshare}"
                    style="@style/txtManropebold14_1" tools:text="@string/msg_care_to_share_more"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_radius_4"
                    android:drawableEnd="@drawable/img_resizing_handle"
                    android:drawablePadding="@dimen/_30pxh"
                    android:hint="@string/msg_leave_your_public"
                    android:id="@+id/etComment" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_1pxh"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@={sendFeedbacksThreeVM.sendFeedbacksThreeModel.etCommentValue}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSubmit"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_submit"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_submit"/>
    </LinearLayout>
</layout>
